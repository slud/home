alias la='ls -la'

goto-matrix()
{
  if [ -d ~/matrix ];
  then
    cd ~/matrix;
    la;
  else
   echo "Katalog matrix nie jest zdefiniowany na tym komputerze.";
  fi
}

goto-farasvn()
{
  cd ~/svn
  la
}

goto-paza()
{
  cd ~/paza
  la
}

hgrep()
{
#	echo "grepping with $1"
	history | grep $1
}

mkcd()
{
	mkdir -p -- "$1" &&
	cd -P -- "$1"
}
